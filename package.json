{
  "name": "maintenance-monorepo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.5.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:frontend": "turbo run dev --filter=frontend",
    "dev:api": "cd packages/api && ./run.sh",
    "dev:all": "concurrently \"npm run dev:api\" \"npm run dev:frontend\"",
    "build": "turbo run build",
    "build:frontend": "turbo run build --filter=frontend",
    "build:api": "turbo run build --filter=api",
    "test": "turbo run test",
    "test:frontend": "turbo run test --filter=frontend",
    "test:api": "cd packages/api && python -m pytest",
    "lint": "turbo run lint --continue",
    "lint:fix": "turbo run lint:fix --continue",
    "clean": "turbo run clean",
    "clean:node_modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "clean:dist": "find . -name 'dist' -type d -prune -exec rm -rf '{}' +",
    "maintenance": "./scripts/maintenance.sh",
    "maintenance:setup": "./scripts/maintenance.sh --setup",
    "maintenance:fix-deps": "./scripts/maintenance.sh --fix-deps",
    "maintenance:fix-db": "./scripts/maintenance.sh --fix-db",
    "maintenance:fix-port": "./scripts/maintenance.sh --fix-port",
    "maintenance:fix-all": "./scripts/maintenance.sh --fix-all",
    "docker:setup": "./scripts/maintenance.sh --docker-setup",
    "docker:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:dev:build": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "docker:test": "docker compose -f docker-compose.yml -f docker-compose.test.yml up",
    "docker:prod": "docker compose -f docker-compose.prod.yml up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:troubleshoot": "./scripts/maintenance.sh --docker-setup",
    "docker:restart": "docker compose restart",
    "docker:rebuild": "docker compose build --no-cache && docker compose up -d",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/file-saver": "^2.0.7",
    "@types/google.maps": "^3.54.10",
    "@types/node": "^20.14.0",
    "@types/react": "^19.0.0",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^19.0.0",
    "@types/recharts": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "axe-core": "^4.10.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "lint-staged": "^15.5.1",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.5.3",
    "terser-webpack-plugin": "^5.3.14",
    "turbo": "^2.5.0",
    "typescript": "^5.4.2",
    "webpack": "^5.99.5",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "vite": "^5.4.18"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@prisma/client": "^5.11.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.10",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.6.3",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "jwt-decode": "^4.0.0",
    "python-shell": "^5.0.0",
    "react": "^19.0.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.22.2",
    "recharts": "^2.15.2",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.17",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  }
}
